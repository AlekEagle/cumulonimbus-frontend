    "build-dev": "vue-tsc --noEmit && vite build -c vite.config.development.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/components": "^8.7.5",
    "@vueuse/core": "^8.6.0",
    "cumulonimbus-wrapper": "^2.0.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.2",
    "vue-tsc": "^0.38.2"
  }
}
